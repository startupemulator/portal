{
  "name": "startupemulator-portal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "husky install",
    "dev": "nuxt",
    "commit": "cz",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js:fix": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore --fix .",
    "lint:style:fix": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore --fix",
    "lint:fix": "npm run lint:js:fix && npm run lint:style:fix",
    "test": "jest",
    "test:e2e": "playwright test",
    "storybook": "nuxt storybook"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/strapi": "^0.3.6",
    "@nuxtjs/toast": "^3.3.1",
    "autoprefixer": "^10.3.7",
    "axios": "^0.23.0",
    "core-js": "^3.18.3",
    "nuxt": "^2.15.8",
    "nuxt-device-detect": "^1.1.5",
    "nuxt-helmet": "^2.0.1",
    "nuxt-property-decorator": "^2.9.1",
    "ts-node": "^10.3.0",
    "vue-spinner": "^1.0.4",
    "vue2-datepicker": "^3.10.2",
    "vue2-touch-events": "^3.2.2",
    "vuelidate": "^0.7.6",
    "vuelidate-property-decorators": "^1.0.28",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/device": "^2.1.0",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/storybook": "^4.2.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg": "^0.3.0",
    "@playwright/test": "^1.17.1",
    "@types/vuelidate": "^0.7.15",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/parser": "^5.1.0",
    "@vue/test-utils": "^1.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-vue": "^7.20.0",
    "fibers": "^5.0.0",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.3",
    "nuxt-purgecss": "^1.0.0",
    "prettier": "^2.5.1",
    "sass": "^1.43.2",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "vue-jest": "^3.0.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
